<h2>商品一覧</h2>
<table>
  <tr>
    <th>商品ID</th>
    <th>商品名</th>
    <th>税抜価格</th>
    <th>ジャンル</th>
    <th>ステータス</th>
  </tr>
  <% @products.each do |product| %>
  <tr>
    <td><%= product.id %></td>
    <td><%= product.name %></td>
    <td><%= product.price %></td>
    <td><%= product.product_type_id %></td>
    <td><%= product.is_active %></td>
  </tr>
  <% end %>
</table>
